{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.16/MicrosoftTeams.schema.json",
    "manifestVersion": "1.16",
    "version": "1.0.0",
    "id": "${{TEAMS_APP_ID}}",
    "packageName": "com.microsoft.teams.extension",
    "developer": {
        "name": "Saad Golandaz",
        "websiteUrl": "https://www.youtube.com/watch?v=dQw4w9WgXcQ",
        "privacyUrl": "https://www.youtube.com/watch?v=dQw4w9WgXcQ",
        "termsOfUseUrl": "https://www.youtube.com/watch?v=dQw4w9WgXcQ"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "Bio Tutor",
        "full": "Biology Smart Tutor"
    },
    "description": {
        "short": "short description for bot",
        "full": "full description for bot"
    },
    "accentColor": "#FFFFFF",
    "bots": [
        {
            "botId": "${{BOT_ID}}",
            "scopes": [
                "personal",
                "team",
                "groupchat"
            ],
            "supportsFiles": false,
            "isNotificationOnly": false
        }
    ],
    "composeExtensions": [],
    "configurableTabs": [],
    "staticTabs": [],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": []
}
